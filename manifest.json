{
    "name": "MyFruit Extension",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css":["style.css"],
        "js": ["fruit.js"]
      }
    ],
    "action": {
      "default_popup": "fruit.html"
    },
  
  "icons": {"16": "logo-16px.png",
            "48": "logo-48px.png",
            "128": "logo-128px.png"

  }
  
  }
  